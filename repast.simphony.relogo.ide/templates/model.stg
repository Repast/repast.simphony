group model;

model_file(modelName) ::= <<
<model name="$modelName$" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://repast.org/scenario/user_path">
	<classpath>
		<agents path="../bin" />
		<entry path="../lib" />
		<builtin fullname="repast.simphony.relogo.WayPoint"/>
		<builtin fullname="repast.simphony.relogo.BaseTurtle"/>
		<builtin fullname="repast.simphony.relogo.BasePatch"/>
	</classpath>
</model>

>>