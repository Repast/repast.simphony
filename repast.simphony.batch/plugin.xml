<plugin>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="repast.eclipse.ide.wizards"
            name="Repast Simphony">
      </category>
      <wizard
            category="repast.eclipse.ide.wizards"
            class="repast.simphony.batch.eclipse.NewBatchParamsFileWizard"
            icon="icons/txt.png"
            id="repast.simphony.batch.wizard"
            name="Batch Parameters File">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.wst.xml.core.catalogContributions">
      <catalogContribution>
         <uri
               name="http://repast.org/batch/params"
               uri="platform:/plugin/repast.simphony.batch/models/batch_params.xsd">
         </uri>
      </catalogContribution>
   </extension>
</plugin>
